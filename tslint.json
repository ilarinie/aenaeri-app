{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-etc"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": true,
        "quotemark": [true, "single"],
        "object-literal-sort-keys": false,
        "space-within-parens": true,
        "no-unused-expression": true,
        "no-unused-declaration": true,
        "indent": [
          true,
          "spaces",
          2
        ]
    },
    "rulesDirectory": []
}