{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-etc",
        "tslint-eslint-rules"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": [
            true,
            "always"
        ],
        "ter-indent": [ true, 4, { "SwitchCase": 1 }],
        "quotemark": [
            true,
            "single"
        ],
        "object-literal-sort-keys": false,
        "space-within-parens": true,
        "no-unused-expression": true,
        "no-unused-declaration": true,
        "interface-name": false,
        "max-line-length": false,
        "no-namespace": false,
        "indent": [
            true,
            "spaces",
            2
        ]
    },
    "rulesDirectory": []
}