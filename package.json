  "scripts": {
    "start": "node dist/index.js",
    "dev": "concurrently \"yarn start-frontend || npm run start-frontend\" \"yarn start-backend || npm run start-backend\"",
    "start-backend": "ts-node-dev --respawn  --project server/tsconfig.json server/index.ts",
    "start-frontend": "react-scripts start",
    "build": "node buildScript.js",
    "build-backend": "tsc -P server/tsconfig.json",
    "build-frontend": "react-scripts build",
    "frontend-test": "react-scripts test",
    "backend-test": "export NODE_ENV=test; export TS_NODE_PROJECT=server/tsconfig.json; nyc mocha --bail --recursive --require ts-node/register \"server/test/**/*.spec.{ts,tsx}\" ",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public .storybook",
    "build-storybook": "build-storybook -s public",
    "create-user": "export NODE_ENV=production; npm run build-backend; node dist/db/createUser.js",
    "populate-database": "npm run build-backend; node bin/updateStatistics.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
